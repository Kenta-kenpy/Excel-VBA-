Sub CreateHTML()
    ' セルA1にタイトルを入力
    Range("A1").Value = "Excel VBA"
    
    ' セルA2にリンクテキストを入力し、セルB2にリンク先URLを入力
    Range("A2").Value = "マクロ"
    Range("B2").Value = "https://github.com/Kenta-kenpy/Excel-VBA-"
    
    ' セルA2のリンクテキストにハイパーリンクを設定
    Range("A2").Hyperlinks.Add Anchor:=Range("A2"), _
        Address:=Range("B2").Value, _
        TextToDisplay:=Range("A2").Value
End Sub
